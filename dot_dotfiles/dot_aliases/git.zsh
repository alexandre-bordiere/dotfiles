alias g='git'
alias ga='git add'
alias gaa='git add --all'
alias gb='git branch'
alias gbd='git branch -D'
alias gc='git commit'
alias gcam='git commit --all --gpg-sign --message'
alias gcamm='git commit --amend --gpg-sign --reuse-message HEAD'
alias gcl='git clone'
alias gcm='git commit --gpg-sign --message'
alias gco='git checkout'
alias gcob='git checkout -b'
alias gd='git diff'
alias gf='git fetch --all --force --prune --tags'
alias gfp='git fetch --all --force --prune --tags && git pull'
alias gl='git log --stat'
alias gp='git push'
alias gpf='git push --force-if-includes --force-with-lease'
alias gpff='git push --force'
alias grb='git rebase'
alias grba='git rebase --abort'
alias grbc='git rebase --continue'
alias grbi='git rebase --interactive'
alias gs='git status'
alias gt='git tag'
alias nuke='git clean -d --force && git reset --hard'
alias pop='git stash pop'
alias stash='git stash --include-untracked'
